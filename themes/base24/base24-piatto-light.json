    "tokenColors": [
        {
            "scope": [
                "emphasis"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "header"
            ],
            "settings": {
                "foreground": "#3c5ea8"
            }
        },
        {
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "#515151"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#c1c1c1"
            }
        },
        {
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#66781e"
            }
        },
        {
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#b23771"
            }
        },
        {
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#cd6f34"
            }
        },
        {
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#b23771",
                "fontStyle": "underline italic"
            }
        },
        {
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#515151",
                "fontStyle": "underline italic"
            }
        },
        {
            "scope": [
                "entity.name.filename"
            ],
            "settings": {
                "foreground": "#cd6f34"
            }
        },
        {
            "scope": [
                "markup.error"
            ],
            "settings": {
                "foreground": "#b23771"
            }
        },
        {
            "name": "Underlined markup",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Bold markup",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#cd6f34"
            }
        },
        {
            "name": "Markup headings",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#3c5ea8"
            }
        },
        {
            "name": "Markup italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#cd6f34",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Bullets, lists (prose)",
            "scope": [
                "beginning.punctuation.definition.list.markdown",
                "beginning.punctuation.definition.quote.markdown",
                "punctuation.definition.link.restructuredtext"
            ],
            "settings": {
                "foreground": "#1e7878"
            }
        },
        {
            "name": "Inline code (prose)",
            "scope": [
                "markup.inline.raw",
                "markup.raw.restructuredtext"
            ],
            "settings": {
                "foreground": "#66781e"
            }
        },
        {
            "name": "Links (prose)",
            "scope": [
                "markup.underline.link",
                "markup.underline.link.image"
            ],
            "settings": {
                "foreground": "#1e7878"
            }
        },
        {
            "name": "Link text, image alt text (prose)",
            "scope": [
                "meta.link.reference.def.restructuredtext",
                "punctuation.definition.directive.restructuredtext",
                "string.other.link.description",
                "string.other.link.title"
